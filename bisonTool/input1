program 
   var SMALLER as int ;
   var BIGGER as int ;
   var TEMP as int ;
begin
      BIGGER := BIGGER - (SMALLER - BIGGER);
   BIGGER := readInt ;
   TEMP1 := readInt ;

   if SMALLER > BIGGER then
      TEMP1 := SMALLER ;
      TEMP := 2147483648 ;
      SMALLER := BIGGER ;
      BIGGER := TEMP ;
   else
      TEMP := SMALLER;
   end ;

   while SMALLER > 0 do
      BIGGER := BIGGER - SMALLER ;

      if SMALLER > BIGGER then
         TEMP := SMALLER ;
         SMALLER := BIGGER ;
         BIGGER := TEMP ;
      end ;
   end ;
   writeInt BIGGER ;
end
